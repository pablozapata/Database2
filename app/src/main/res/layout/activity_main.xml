<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="de.uni_s.ipvs.mcl.assignment5.MainActivity">


    <TextView
        android:id="@+id/suscribe_res"
        android:layout_width="82dp"
        android:layout_height="28dp"
        android:text="..."
        android:layout_marginRight="94dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/subscribe_time"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="1.0" />

    <Button
        android:id="@+id/suscribe"
        android:layout_width="125dp"
        android:layout_height="54dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="Suscribe to changes"
        app:layout_constraintHorizontal_bias="0.032"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="34dp"
        app:layout_constraintTop_toBottomOf="@+id/readLast" />

    <Button
        android:id="@+id/readLast"
        android:layout_width="131dp"
        android:layout_height="51dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="Read last value"
        app:layout_constraintHorizontal_bias="0.035"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="34dp"
        app:layout_constraintTop_toBottomOf="@+id/writevalue" />

    <Button
        android:id="@+id/connection"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Connect to device"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="57dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintHorizontal_bias="0.543" />

    <Button
        android:id="@+id/average"
        android:layout_width="120dp"
        android:layout_height="54dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="Get day average"
        app:layout_constraintHorizontal_bias="0.032"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/suscribe"
        app:layout_constraintVertical_bias="0.189" />

    <TextView
        android:id="@+id/average_res"
        android:layout_width="133dp"
        android:layout_height="34dp"
        android:text="..."
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="165dp"
        app:layout_constraintLeft_toRightOf="@+id/average"
        android:layout_marginLeft="37dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.228"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/suscribe_res"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/readLast_res"
        android:layout_width="68dp"
        android:layout_height="24dp"
        android:text="..."
        android:layout_marginTop="18dp"
        app:layout_constraintTop_toBottomOf="@+id/readLast_time"
        android:layout_marginRight="95dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/readLast"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="1.0" />

    <Button
        android:id="@+id/writevalue"
        android:layout_width="114dp"
        android:layout_height="50dp"
        android:text="writevalue"
        android:layout_marginLeft="28dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="37dp"
        app:layout_constraintRight_toLeftOf="@+id/tempRealT"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/tempRealT"
        android:layout_width="97dp"
        android:layout_height="29dp"
        android:text="..."
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="45dp"
        android:layout_marginRight="78dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/subscribe_time"
        android:layout_width="128dp"
        android:layout_height="28dp"
        android:text="..."
        android:layout_marginRight="62dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.973"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/readLast_res" />

    <TextView
        android:id="@+id/readLast_time"
        android:layout_width="124dp"
        android:layout_height="34dp"
        android:text="..."
        android:layout_marginTop="35dp"
        app:layout_constraintTop_toBottomOf="@+id/tempRealT"
        android:layout_marginRight="55dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="1.0" />


</android.support.constraint.ConstraintLayout>


